FoamFile
{
    version     2.0;
    format      binary;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 440.15;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 440.15;
    }
    outlet
    {
        type            zeroGradient;
    }
    airSlip
    {
        type            slip;
    }
    suzanne
    {
        type            omegaWallFunction;
        value           uniform 440.15;
    }
}


